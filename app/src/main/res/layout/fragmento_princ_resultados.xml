<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:context=".principal.FragmentoPrincResultados"
        android:padding="16dp">

    <Spinner
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/spinner_jogo" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" tools:layout_editor_absoluteX="16dp"/>
    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/spinner_jogo" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:padding="8dp"
            android:id="@+id/linearLayout_dez" android:layout_marginTop="8dp">

        <TextView
                android:id="@+id/textView01"
                android:text="09"
                android:layout_width="@dimen/tamanho_dezenhas"
                android:layout_height="@dimen/tamanho_dezenhas"
                android:layout_weight="1"
                android:layout_margin="4dp"
                android:padding="8dp"
                android:background="@drawable/shape_redondo"
                android:textColor="@color/branco" android:textStyle="bold" android:textSize="25sp"
                android:backgroundTint="@color/cor_megasena_1"/>

        <TextView
                android:id="@+id/textViewNum02"
                android:text="15"
                android:layout_width="@dimen/tamanho_dezenhas"
                android:layout_height="@dimen/tamanho_dezenhas"
                android:layout_weight="1"
                android:layout_margin="4dp"
                android:padding="8dp"
                android:background="@drawable/shape_redondo"
                android:textColor="@color/branco" android:textStyle="bold" android:textSize="25sp"
                android:backgroundTint="@color/cor_megasena_1"/>

        <TextView
                android:id="@+id/textViewNum03"
                android:text="26"
                android:layout_width="@dimen/tamanho_dezenhas"
                android:layout_height="@dimen/tamanho_dezenhas"
                android:layout_weight="1"
                android:layout_margin="4dp"
                android:padding="8dp"
                android:background="@drawable/shape_redondo"
                android:textColor="@color/branco" android:textStyle="bold" android:textSize="25sp"
                android:backgroundTint="@color/cor_megasena_1"/>

        <TextView
                android:id="@+id/textViewNum04"
                android:text="31"
                android:layout_width="@dimen/tamanho_dezenhas"
                android:layout_height="@dimen/tamanho_dezenhas"
                android:layout_weight="1"
                android:layout_margin="4dp"
                android:padding="8dp"
                android:background="@drawable/shape_redondo"
                android:textColor="@color/branco" android:textStyle="bold" android:textSize="25sp"
                android:backgroundTint="@color/cor_megasena_1"/>

        <TextView
                android:id="@+id/textViewNum05"
                android:text="49"
                android:layout_width="@dimen/tamanho_dezenhas"
                android:layout_height="@dimen/tamanho_dezenhas"
                android:layout_weight="1"
                android:layout_margin="4dp"
                android:padding="8dp"
                android:background="@drawable/shape_redondo"
                android:textColor="@color/branco" android:textStyle="bold" android:textSize="25sp"
                android:backgroundTint="@color/cor_megasena_1"/>

        <TextView
                android:id="@+id/textViewNum06"
                android:text="53"
                android:layout_width="@dimen/tamanho_dezenhas"
                android:layout_height="@dimen/tamanho_dezenhas"
                android:layout_weight="1"
                android:layout_margin="4dp"
                android:padding="8dp"
                android:background="@drawable/shape_redondo"
                android:textColor="@color/branco" android:textStyle="bold" android:textSize="25sp"
                android:backgroundTint="@color/cor_megasena_1"/>
    </LinearLayout>
    <TextView
            android:id="@+id/textViewNumConcurso"
            android:text="Consurso: 1569"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout_dez"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:layout_marginTop="8dp"/>
</android.support.constraint.ConstraintLayout>